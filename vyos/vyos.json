{
	"builders": [{
			"type": "qemu",
			"cpus": 2,
			"memory": 2048,
			"net_device": "virtio-net",
			"headless": "true",
			"iso_url": "https://s3-us.vyos.io/rolling/current/vyos-rolling-latest.iso",
			"iso_checksum": "none",
			"output_directory": "/tmp/output-vyos-rolling-amd64",
			"vm_name": "vyos-{{isotime `20060102`}}.img",
			"disk_size": "20000",
			"disk_compression": true,
			"boot_wait": "10m",
			"communicator": "ssh",
			"ssh_timeout": "2m",
			"ssh_username": "vyos",
			"ssh_password": "vyos",
			"shutdown_command": "sudo shutdown -h now",
			"boot_command": [
				"vyos<enter><wait5>",
				"vyos<enter><wait10>",
				"install image<enter><wait5>",
				"Yes<enter><wait5>",
				"Auto<enter><wait5>",
				"<enter><wait5>",
				"Yes<enter><wait5>",
				"<enter><wait1m>",
				"<enter><wait1m>",
				"<enter><wait10>",
				"vyos<enter><wait5>",
				"vyos<enter><wait5>",
				"<enter><wait10>",
				"configure<enter><wait5>",
				"set interfaces ethernet eth0 address dhcp<enter><wait5>",
				"set service ssh<enter><wait5>",
				"<wait5>",
				"commit<enter><wait10>"
			]
		}
	]
}
