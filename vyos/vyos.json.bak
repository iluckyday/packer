{
	"builders": [{
			"type": "qemu",
			"cpus": 2,
			"memory": 2048,
			"net_device": "virtio-net",
			"headless": "true",
			"iso_url": "https://s3-us.vyos.io/rolling/current/vyos-rolling-latest.iso",
			"iso_checksum": "none",
			"output_directory": "/tmp/output-vyos-rolling-amd64",
			"vm_name": "vyos-{{isotime `20060102`}}.img",
			"disk_size": "20000",
			"disk_compression": true,
			"boot_wait": "10m",
			"boot_command": [
				"vyos<enter><wait5>",
				"vyos<enter><wait10>",
				"install image<enter><wait5>",
				"Yes<enter><wait5>",
				"Auto<enter><wait5>",
				"<enter><wait5>",
				"Yes<enter><wait5>",
				"<enter><wait1m>",
				"<enter><wait1m>",
				"<enter><wait10>",
				"vyos<enter><wait5>",
				"vyos<enter><wait5>",
				"<enter><wait10>",
				"reboot<enter><wait5>",
				"Yes<enter><wait10m>",
				"vyos<enter><wait5>",
				"vyos<enter><wait10>",
				"configure<enter><wait5>",
				"set interfaces ethernet eth0 address dhcp<enter><wait5>",
				"set interfaces ethernet eth0 description 'Management'<enter><wait5>",
				"set interfaces ethernet eth0 vrf 'management'<enter><wait5>",
				"set service ssh vrf 'management'<enter><wait5>",
				"set system login user vyos authentication public-keys vyos key 'AAAAC3NzaC1lZDI1NTE5AAAAIDyuzRtZAyeU3VGDKsGk52rd7b/rJ/EnT8Ce2hwWOZWp'<enter><wait5>",
				"set system login user vyos authentication public-keys vyos type 'ssh-ed25519'<enter><wait5>",
				"set vrf name management table '65530'<enter><wait5>",
				"<wait5>",
				"delete interfaces ethernet eth0 hw-id<enter><wait5>",
				"<wait5>",
				"commit<enter><wait10>",
				"save<enter><wait5>",
				"exit<enter><wait5>",
				"exit<enter><wait5>"
			],
			"ssh_timeout": "2m",
			"ssh_username": "vyos",
			"ssh_password": "vyos",
			"shutdown_command": "sudo shutdown -h now"
		}
	]
}
